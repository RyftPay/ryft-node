{
  "compilerOptions": {
    "outDir": "./dist",
    "module": "commonjs",
    "target": "es2022",
    "declaration": true,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "rootDir": "src",
    "emitDeclarationOnly": true,
    "strictPropertyInitialization": false
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}